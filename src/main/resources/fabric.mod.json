{
	"schemaVersion": 1,
	"id": "onedaymod",
	"version": "${version}",
	"name": "One Day Mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Kaupenjoe"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/onedaymod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.kaupenjoe.onedaymod.OneDayMod"
		],
		"client": [
			"net.kaupenjoe.onedaymod.OneDayClientMod"
		],
		"fabric-datagen": [
			"net.kaupenjoe.onedaymod.OneDayModDataGenerator"
		]
	},
	"accessWidener" : "onedaymod.accesswidener",
	"mixins": [
		"onedaymod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.7",
		"minecraft": "~1.20.1",
		"java": ">=17",
